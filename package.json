{
	"name": "ga4-mixpanel",
	"version": "1.0.0",
	"description": "ETL data from Google Analytics 4 to Mixpanel (through bigquey and cloud storage)",
	"main": "index.js",
	"scripts": {
		"start": "functions-framework --target=go --source . --signature-type=http --port=8080",
		"deploy:func": "sh deploy_cloud_functions.sh",
		"deploy:run": "sh deploy_cloud_run.sh",
		"test": "node --no-warnings --experimental-vm-modules node_modules/jest/bin/jest.js",
		"coverage": "node --no-warnings --experimental-vm-modules node_modules/jest/bin/jest.js --coverage && open ./tests/coverage/lcov-report/index.html",
		"dev": "RUNTIME=dev nodemon --exec 'functions-framework --target=go --source . --signature-type=http | bunyan -o short'",
		"mock": "functions-framework --target=go --source . --signature-type=http 2>&1 | bunyan -o short"
	},
	"keywords": [],
	"author": "ak@mixpanel.com",
	"license": "ISC",
	"type": "module",
	"dependencies": {
		"@google-cloud/bigquery": "^7.3.0",
		"@google-cloud/functions-framework": "^3.3.0",
		"@google-cloud/storage": "^7.7.0",
		"ak-tools": "^1.0.49",
		"dayjs": "^1.11.10",
		"dotenv": "^16.3.1",
		"mixpanel-import": "^2.5.30",
		"p-limit": "^5.0.0"
	},
	"devDependencies": {
		"@jest/globals": "^29.7.0",
		"@types/jest": "^29.5.11",
		"bunyan": "^1.8.15",
		"jest": "^29.7.0",
		"nock": "^13.4.0",
		"tree-kill": "^1.2.2"
	}
}
